package com.csci4060.app.model;

import lombok.Data;
import lombok.RequiredArgsConstructor;

@Data

public class APIresponse {
	
	private int status;
	private String message;
	private Object result;
	public APIresponse(int value, String string, Object jwt) {
		this.status = value;
		this.message = string;
		this.result = jwt;
	}
	
}
